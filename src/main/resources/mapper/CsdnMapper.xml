<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.luo.webmagic.mapper.CsdnMapper" >
  <resultMap id="csdn" type="com.luo.webmagic.entity.Csdn" >
        <id column="id" property="id" />
        <result column="title" property="title"/>
        <result column="time" property="time"/>
        <result column="author" property="author"/>
        <result column="authorUrl" property="authorUrl"/>
        <result column="label" property="label"/>
        <result column="reading" property="reading"/>
      <result column="url" property="url"/>
  </resultMap>

    <insert id="insert" parameterType="com.luo.webmagic.entity.Csdn" >
    insert into csdn
    VALUES
    (
    UUID(),
     #{title},
     #{time},
     #{author},
     #{authorUrl},
     #{label},
     #{reading},
     #{url}
     )
  </insert>

</mapper>